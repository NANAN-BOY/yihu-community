<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yihu.mapper.MemberShipMapper">
    <select id="selectByUserID" resultType="com.yihu.entity.MemberShip">
        select *
        from membership
        where user_id = #{userId}
    </select>
</mapper>